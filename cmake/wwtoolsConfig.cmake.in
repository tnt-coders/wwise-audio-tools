@PACKAGE_INIT@

# Find required dependencies
include(CMakeFindDependencyMacro)

# These dependencies are required for linking
find_dependency(Ogg REQUIRED)
find_dependency(Vorbis REQUIRED)

# Include the exported targets
include("${CMAKE_CURRENT_LIST_DIR}/wwtoolsTargets.cmake")

# Provide alias targets for compatibility if not already defined
if(NOT TARGET wwtools::shared AND TARGET wwtools-shared-lib)
    add_library(wwtools::shared ALIAS wwtools-shared-lib)
endif()

if(NOT TARGET wwtools::static AND TARGET wwtools-static-lib)
    add_library(wwtools::static ALIAS wwtools-static-lib)
endif()

check_required_components(wwtools)
