{
  "version": 7,
  "configurePresets": [
    {
      "name": "debug",
      "displayName": "Debug",
      "description": "Debug build configuration with Qt Creator auto-setup disabled",
      "binaryDir": "${sourceDir}/build/debug",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_PROJECT_TOP_LEVEL_INCLUDES": "${sourceDir}/cmake-conan/conan_provider.cmake",
        "CONAN_INSTALL_BUILD_CONFIGURATIONS": "Debug",
        "QT_CREATOR_SKIP_PACKAGE_MANAGER_SETUP": true
      }
    },
    {
      "name": "release",
      "displayName": "Release",
      "description": "Release build configuration with Qt Creator auto-setup disabled",
      "binaryDir": "${sourceDir}/build/release",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "CMAKE_PROJECT_TOP_LEVEL_INCLUDES": "${sourceDir}/cmake-conan/conan_provider.cmake",
        "CONAN_INSTALL_BUILD_CONFIGURATIONS": "Release",
        "QT_CREATOR_SKIP_PACKAGE_MANAGER_SETUP": true
      }
    }
  ],
  "buildPresets": [
    {
      "name": "debug",
      "configurePreset": "debug",
      "configuration": "Debug"
    },
    {
      "name": "release",
      "configurePreset": "release",
      "configuration": "Release"
    }
  ],
  "testPresets": [
    {
      "name": "debug",
      "configurePreset": "debug",
      "configuration": "Debug"
    },
    {
      "name": "release",
      "configurePreset": "release",
      "configuration": "Release"
    }
  ]
}
