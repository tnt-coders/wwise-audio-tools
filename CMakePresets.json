{
  "version": 7,
  "include": [
    "cmake-config/presets/default.json",
    "cmake-config/presets/conan.json"
  ],
  "configurePresets": [
    {
      "name": "debug",
      "inherits": ["default-debug", "conan-debug"]
    },
    {
      "name": "release",
      "inherits": ["default-release", "conan-release"]
    }
  ],
  "buildPresets": [
    {
      "name": "debug",
      "inherits": ["default-debug", "conan-debug"]
    },
    {
      "name": "release",
      "inherits": ["default-release", "conan-release"]
    }
  ],
  "testPresets": [
    {
      "name": "debug",
      "inherits": ["default-debug", "conan-debug"]
    },
    {
      "name": "release",
      "inherits": ["default-release", "conan-release"]
    }
  ]
}
